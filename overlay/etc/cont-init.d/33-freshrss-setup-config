#!/usr/bin/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If FRESHRSS_SALT is empty, we cannot proceed.
#======================================================================================================================

[[ -z "${FRESHRSS_SALT-}" ]] && bf-error "${FRESHRSS_SALT} is not defined."


#======================================================================================================================
# Run setup config executable.
#======================================================================================================================

${FRESHRSS_LIB}/setup-config
freshrss-setup-complete
