#!/usr/bin/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If FRESHRSS_DATA is empty, the content setup has not worked correctly.
#======================================================================================================================

[[ -z "$(ls -A ${FRESHRSS_DATA})" ]] && bf-error "${FRESHRSS_DATA} is empty."


#======================================================================================================================
# Run setup content executable.
#======================================================================================================================

${FRESHRSS_LIB}/setup-install
freshrss-setup-complete
