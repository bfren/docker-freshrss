#!/usr/bin/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# If FRESHRSS_SRC doesn't exist, the source files have not been downloaded correctly.
#======================================================================================================================

[[ ! -d ${FRESHRSS_SRC} ]] && bf-error "FreshRSS source files cannot be found."


#======================================================================================================================
# Run setup content executable.
#======================================================================================================================

${FRESHRSS_LIB}/setup-data
